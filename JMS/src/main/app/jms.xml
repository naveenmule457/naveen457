    <jms:activemq-connector name="Active_MQ" brokerURL="tcp://localhost:61616" validateConnections="true" doc:name="Active MQ"/>
    <http:listener-config name="HTTP_Listener_Configuration" host="localhost" port="8081" doc:name="HTTP Listener Configuration"/>
    <jms:activemq-connector name="Active_MQ2" brokerURL="tcp://localhost:61616" validateConnections="true" doc:name="Active MQ"/>
    <flow name="jmsFlowforsendingtothequeue">
        <http:listener config-ref="HTTP_Listener_Configuration" path="/tt" doc:name="HTTP"/>
   <set-payload value="Test Data has been completed successfullly for third time" doc:name="Set Payload"/>
        <jms:outbound-endpoint connector-ref="Active_MQ" doc:name="JMS" queue="naveen">
            <jms:transaction action="NONE"/>
        </jms:outbound-endpoint>
        <set-payload value="testing has been completed successfully" doc:name="Set Payload"/>
    </flow>
    
</mule>
